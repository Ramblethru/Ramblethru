<%= simple_form_for Discover.new do |f| %>
    <%= f.input :destination, label: false, placeholder: "Destination" %>
    <%= f.text_field :start_date, label: false, placeholder: "Start Date",id: 'start_date' %> 
    <%= f.text_field :end_date,label: false, placeholder: "End Date", id: 'end_date' %>
    <%= button_to 'Save this Ramble', { :controller => 'rambles', :action => 'create'} %>

<% end %>