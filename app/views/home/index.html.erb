    <%= render "/discovers/create" %>
  </div>
</div>

<div class="container">
	<div class="homepage-about"><h1 class="what-we-do">What We Do</h1>
		<div id="about">Something here.</div>
	</div>
		<article>
		</article>

	<section class="homepage-about-discover">
		<div class="discover">
			<h2 class="discover">Discover</h2>
