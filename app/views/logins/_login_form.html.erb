<div class="login-form">
	<%= form_for(:session, url: logins_path) do |f| %>
	  <%= f.label :email, :class => "login-form" %>
	  <%= f.email_field :email, :class => "login-form" %>
	  <%= f.label :password, :class => "login-form" %>
	  <%= f.password_field :password, :class => "login-form" %>
	  <%= f.button :submit, :class => "login-button" %>
	<% end %>
	<%= link_to "Sign in with Foursquare", "/auth/foursquare", :class => "four-auth" %>
	<%= link_to "Sign in with Twitter", "/auth/twitter", :class => "twit-auth" %>

</div>
