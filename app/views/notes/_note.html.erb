<div class="row" id="<%= note.persisted? ? dom_id(note) : "note-new" %>">

	<div class="notebody">
    Note: <%= note.body %>
    <p>Tag: <%= note.tag_list %> </p>
    <%= link_to 'Edit note', edit_ramble_note_path(@ramble.id, note.id) %> ,
      <%= link_to 'Destroy', note, method: :delete, remote: true %>
	</div>

</div>
