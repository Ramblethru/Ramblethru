<%= simple_form_for [@ramble, @ramble.notes.build], html: {id: 'noteform' }, remote: true do |f| %>
    <%= f.input :title, label: false, placeholder: "Your note's title", :class => "note-body" %>
    <%= f.input :address, label: false, placeholder: "Venue address", :class => "note-body" %>
    <%= f.input :body, label: false, placeholder: "Your body text (REQUIRED)", :class => "note-body" %>
    <%= f.input :url, label: false, placeholder: "URL", :class => "note-body" %>
    <%= f.input :tag_list, label: false, :class=>"tag-input", :placeholder=>"Tags", :class => "note-body" %>
    <%= f.button :submit, "Add note", :class => "radius", id: "submit_note" %>
<% end %>
