<%= simple_form_for [@ramble, @ramble.notes.build], remote: true do |f| %>
    <%= f.input :body, label: false, placeholder: "Your body text", :class => "note-body" %>
    <%= f.input :tag_list, label: false, :class=>"tag-input", :placeholder=>"Tags", :class => "note-body" %>
    <%= f.button :submit, "Add note", :class => "radius" %>
<% end %>
