<div class="row">

	<div class="notebody">
		<% if note.body %>
		<p>
			<span id="note-label">Title: </span>
			<span id="note-title"><%= note.title %></span>
		</p>
		<p>
			<span id="note-label">Address: </span>
			<span id="note-address"><%= note.address %></span>
		</p>
		<p>
			<span id="note-label">Note: </span>
			<span id="note-body"><%= note.body %></span>
		</p>
		<p>
			<span id="note-label">URL: </span>
			<span id="note-url"><a href="<%= note.url %>" target="_blank"><%= note.url %></a></span>
		</p>
		<p>
			<span id="tag-label">Tag(s): </span>
			<span id="note-tag"><%= note.tag_list %></span>
		</p>
		<p>
			<span id="note-label">Created on: </span>
			<span id="note-created"> <%= note.created_at.strftime("%m/%d/%Y") %>
			</p>
		</div>
	</div>
