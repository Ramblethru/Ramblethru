<div class="row" id="<%= ramble.persisted? ? dom_id(ramble) : "ramble-new" %>">
	<div class="container">
		<% if logged_in? %>
		<% if ramble.user_id != current_user.id %>
		<% ramble.notes.each do |note| %>
		<% if note.share %>
		<div class="note-results">
          <% if note.title != "" %>
			<div class="notesshow">
				Title: <%= note.title %>
			</div>
              <% end %>
          <% if note.address =! "" %>
			<div class="notesshow">
				Address: <%= note.address %>
			</div>
              <% end %>
          <% if note.body != "" %>
			<div class="notesshow">
				Note: <%= note.body %>
			</div>
              <% end %>
          <% if note.url != "" %>
			<div class="notesshow">
				URL: <a href="<%= note.url %>" target="_blank"><%= note.url %></a>
			</div>
              <% end %>
          <% if note.tag_list != [] %>
			<div class="tagshow">
				Tags: <%= note.tag_list %>
			</div>
              <% end %>
			<div class="notesshow">
				Created on: <%= note.created_at.strftime("%m/%d/%Y") %>
			</div>
			<div class="note-destination">
				<%= ramble.destination.upcase %>
			</div>
		</div>
		<% end %>
		<% end %>
		<% end %>
		<% else %>
		<% ramble.notes.each do |note| %>
		<% if note.share %>
		<div class="note-results">
          <% if note.title != "" %>
              <div class="notesshow">
                Title: <%= note.title %>
              </div>
          <% end %>
          <% if note.address =! "" %>
              <div class="notesshow">
                Address: <%= note.address %>
              </div>
          <% end %>
          <% if note.body != "" %>
              <div class="notesshow">
                Note: <%= note.body %>
              </div>
          <% end %>
          <% if note.url != "" %>
              <div class="notesshow">
                URL: <a href="<%= note.url %>" target="_blank"><%= note.url %></a>
              </div>
          <% end %>
          <% if note.tag_list != [] %>
              <div class="tagshow">
                Tags: <%= note.tag_list %>
              </div>
          <% end %>
          <div class="notesshow">
            Created on: <%= note.created_at.strftime("%m/%d/%Y") %>
          </div>
          <div class="note-destination">
            <%= ramble.destination.upcase %>
          </div>
        </div>
		<% end %>
		<% end %>
		<% end %>
	</div>
</div>
